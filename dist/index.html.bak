<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>üéÆ Flappy Meme Game üéÆ</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen',
                'Ubuntu', 'Cantarell', 'Fira Sans', 'Droid Sans', 'Helvetica Neue',
                sans-serif;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }
    </style>
</head>
<body>
    <div id="root"></div>
    <script>
        "use strict";

        const MEME_FACES = [
            { emoji: "üòé", name: "Cool Doge" },
            { emoji: "üê∏", name: "Pepe" },
            { emoji: "üí™", name: "Chad" },
            { emoji: "üò≠", name: "Crying Wojak" },
            { emoji: "ü§°", name: "Clown" },
            { emoji: "üíÄ", name: "Skull" },
            { emoji: "üóø", name: "Moai" },
            { emoji: "üß†", name: "Brain" },
        ];

        const THEMES = {
            night: {
                bg: "from-[#0f0f1e] to-[#1a1a2e]",
                canvasGradient: ["#1a1a2e", "#16213e", "#0f1419"],
                particle: "rgba(255, 255, 255, 0.8)",
                particleType: "star",
                pipeGradient: ["#2a2a3e", "#1f1f2e"],
                pipeBorder: "#404055",
                title: "#e0e0e0",
                text: "#b0b0b0",
                textLight: "#a0a0a0",
                canvasBorder: "#2a2a3e",
                button: "#2a2a3e",
                buttonHover: "#3a3a4e",
                buttonBorder: "#404055",
                button2: "#3a3a4e",
                button2Hover: "#4a4a5e",
                button2Border: "#505065",
                buttonDanger: "#4a2a2a",
                buttonDangerHover: "#5a3a3a",
                buttonDangerBorder: "#6a4a4a",
            },
            day: {
                bg: "from-[#87CEEB] to-[#E0F6FF]",
                canvasGradient: ["#87CEEB", "#B0E2FF", "#E0F6FF"],
                particle: "rgba(255, 255, 255, 0.9)",
                particleType: "cloud",
                pipeGradient: ["#90EE90", "#76D676"],
                pipeBorder: "#5CB85C",
                title: "#2c3e50",
                text: "#34495e",
                textLight: "#546e7a",
                canvasBorder: "#5dade2",
                button: "#3498db",
                buttonHover: "#2980b9",
                buttonBorder: "#2471a3",
                button2: "#27ae60",
                button2Hover: "#229954",
                button2Border: "#1e8449",
                buttonDanger: "#e74c3c",
                buttonDangerHover: "#c0392b",
                buttonDangerBorder: "#a93226",
            },
        };

        let gameState = "start";
        let score = 0;
        let highScore = localStorage.getItem('flappyMemeHighScore') || 0;
        let currentMeme = 0;
        let customImage = null;
        let customMusic = null;
        let customText = "";
        let showTextInput = false;
        let theme = "night";
        let canvasSize = { width: 800, height: 500 };

        let gameRef = {
            bird: { x: 100, y: 250, velocity: 0, radius: 25 },
            pipes: [],
            frame: 0,
            gravity: 0.5,
            jumpStrength: -9,
            pipeSpeed: 3,
            pipeGap: 220,
            pipeWidth: 60,
            scored: new Set(),
        };

        let audioContextRef = null;
        let customAudioRef = null;
        let imageRef = null;
        let animationFrameRef = null;

        function getCurrentTheme() {
            return THEMES[theme];
        }

        function handleImageUpload(e) {
            const file = e.target.files?.[0];
            if (file && file.type.startsWith("image/")) {
                const reader = new FileReader();
                reader.onload = (event) => {
                    const img = new Image();
                    img.onload = () => {
                        imageRef = img;
                        customImage = event.target.result;
                        render();
                    };
                    img.src = event.target.result;
                };
                reader.readAsDataURL(file);
            }
        }

        function handleMusicUpload(e) {
            const file = e.target.files?.[0];
            if (file && file.type.startsWith("audio/")) {
                // Stop previous music if playing
                if (customAudioRef) {
                    customAudioRef.pause();
                    customAudioRef = null;
                }
                customMusic = URL.createObjectURL(file);
                render();
            }
        }

        function playSound(type) {
            if (!audioContextRef) return;

            const ctx = audioContextRef;
            const oscillator = ctx.createOscillator();
            const gainNode = ctx.createGain();

            oscillator.connect(gainNode);
            gainNode.connect(ctx.destination);

            if (type === "jump") {
                oscillator.frequency.value = 400;
                gainNode.gain.setValueAtTime(0.1, ctx.currentTime);
                gainNode.gain.exponentialRampToValueAtTime(0.01, ctx.currentTime + 0.1);
                oscillator.start(ctx.currentTime);
                oscillator.stop(ctx.currentTime + 0.1);
            } else if (type === "score") {
                oscillator.frequency.value = 800;
                gainNode.gain.setValueAtTime(0.1, ctx.currentTime);
                gainNode.gain.exponentialRampToValueAtTime(0.01, ctx.currentTime + 0.2);
                oscillator.start(ctx.currentTime);
                oscillator.stop(ctx.currentTime + 0.2);
            } else if (type === "die") {
                oscillator.frequency.setValueAtTime(400, ctx.currentTime);
                oscillator.frequency.exponentialRampToValueAtTime(100, ctx.currentTime + 0.3);
                gainNode.gain.setValueAtTime(0.2, ctx.currentTime);
                gainNode.gain.exponentialRampToValueAtTime(0.01, ctx.currentTime + 0.3);
                oscillator.start(ctx.currentTime);
                oscillator.stop(ctx.currentTime + 0.3);
            }
        }

        function startGame() {
            gameRef = {
                bird: { x: 100, y: 250, velocity: 0, radius: 25 },
                pipes: [],
                frame: 0,
                gravity: 0.5,
                jumpStrength: -9,
                pipeSpeed: 3,
                pipeGap: 180,
                pipeWidth: 60,
                scored: new Set(),
            };
            score = 0;
            gameState = "playing";
            startGameLoop();
            render();
        }

        function jump() {
            if (gameState === "start") {
                startGame();
            } else if (gameState === "playing") {
                gameRef.bird.velocity = gameRef.jumpStrength;
                playSound("jump");
            }
        }

        function changeMeme() {
            currentMeme = (currentMeme + 1) % MEME_FACES.length;
            customImage = null;
            imageRef = null;
            render();
        }

        function toggleTheme() {
            theme = theme === "night" ? "day" : "night";
            render();
        }

        function drawCanvasBackground() {
            const canvas = document.getElementById('gameCanvas');
            if (!canvas) return;
            const ctx = canvas.getContext("2d");
            const currentTheme = getCurrentTheme();
            
            const gradient = ctx.createLinearGradient(0, 0, 0, canvasSize.height);
            gradient.addColorStop(0, currentTheme.canvasGradient[0]);
            gradient.addColorStop(0.5, currentTheme.canvasGradient[1]);
            gradient.addColorStop(1, currentTheme.canvasGradient[2]);
            ctx.fillStyle = gradient;
            ctx.fillRect(0, 0, canvasSize.width, canvasSize.height);
            
            ctx.fillStyle = currentTheme.particle;
            for (let i = 0; i < 50; i++) {
                const x = ((gameRef.frame * 0.2 + i * 123) % (canvasSize.width + 50)) - 50;
                const y = (i * 67) % canvasSize.height;
                if (currentTheme.particleType === "star") {
                    ctx.fillRect(x, y, 2, 2);
                } else {
                    ctx.beginPath();
                    ctx.arc(x, y, 15, 0, Math.PI * 2);
                    ctx.arc(x + 20, y, 20, 0, Math.PI * 2);
                    ctx.arc(x + 40, y, 15, 0, Math.PI * 2);
                    ctx.fill();
                }
            }
        }

        function startGameLoop() {
            const canvas = document.getElementById('gameCanvas');
            if (!canvas) return;

            const ctx = canvas.getContext("2d");

            // Play background music if uploaded
            if (customMusic && !customAudioRef) {
                try {
                    const audio = new Audio(customMusic);
                    audio.loop = true;
                    audio.volume = 0.3;
                    audio.play().catch(err => console.log("Music play failed:", err));
                    customAudioRef = audio;
                } catch (err) {
                    console.error("Error playing music:", err);
                }
            }

            const gameLoop = () => {
                if (gameState !== "playing") {
                    if (animationFrameRef) {
                        cancelAnimationFrame(animationFrameRef);
                        animationFrameRef = null;
                    }
                    // Render game over screen only when state changes to gameOver
                    if (gameState === "gameOver") {
                        render();
                    }
                    return;
                }

                gameRef.frame++;
                const game = gameRef;
                const currentTheme = getCurrentTheme();

                // Clear canvas
                const gradient = ctx.createLinearGradient(0, 0, 0, canvasSize.height);
                gradient.addColorStop(0, currentTheme.canvasGradient[0]);
                gradient.addColorStop(0.5, currentTheme.canvasGradient[1]);
                gradient.addColorStop(1, currentTheme.canvasGradient[2]);
                ctx.fillStyle = gradient;
                ctx.fillRect(0, 0, canvasSize.width, canvasSize.height);

                // Draw particles
                ctx.fillStyle = currentTheme.particle;
                for (let i = 0; i < 50; i++) {
                    const x = ((game.frame * 0.2 + i * 123) % (canvasSize.width + 50)) - 50;
                    const y = (i * 67) % canvasSize.height;
                    if (currentTheme.particleType === "star") {
                        ctx.fillRect(x, y, 2, 2);
                    } else {
                        ctx.beginPath();
                        ctx.arc(x, y, 15, 0, Math.PI * 2);
                        ctx.arc(x + 20, y, 20, 0, Math.PI * 2);
                        ctx.arc(x + 40, y, 15, 0, Math.PI * 2);
                        ctx.fill();
                    }
                }

                // Update bird physics
                game.bird.velocity += game.gravity;
                game.bird.y += game.bird.velocity;

                // Spawn pipes
                if (game.frame % 90 === 0) {
                    let gapY = Math.random() * (500 - game.pipeGap - 80) + 40;
                    // Ensure gap is not in center where bird starts
                    while (gapY >= 150 && gapY <= 350) {
                        gapY = Math.random() * (500 - game.pipeGap - 80) + 40;
                    }
                    game.pipes.push({
                        x: 800,
                        gapY: gapY,
                        id: game.frame,
                    });
                }

                // Update pipes
                game.pipes = game.pipes.filter((pipe) => {
                    pipe.x -= game.pipeSpeed;

                    const gradient1 = ctx.createLinearGradient(
                        pipe.x,
                        0,
                        pipe.x + game.pipeWidth,
                        0,
                    );
                    gradient1.addColorStop(0, currentTheme.pipeGradient[0]);
                    gradient1.addColorStop(1, currentTheme.pipeGradient[1]);

                    ctx.fillStyle = gradient1;
                    ctx.fillRect(
                        pipe.x,
                        0,
                        game.pipeWidth,
                        pipe.gapY,
                    );
                    ctx.strokeStyle = currentTheme.pipeBorder;
                    ctx.lineWidth = 3;
                    ctx.strokeRect(
                        pipe.x,
                        0,
                        game.pipeWidth,
                        pipe.gapY,
                    );

                    ctx.fillRect(
                        pipe.x,
                        pipe.gapY + game.pipeGap,
                        game.pipeWidth,
                        500 - pipe.gapY - game.pipeGap,
                    );
                    ctx.strokeRect(
                        pipe.x,
                        pipe.gapY + game.pipeGap,
                        game.pipeWidth,
                        500 - pipe.gapY - game.pipeGap,
                    );

                    // Check score
                    if (
                        !game.scored.has(pipe.id) &&
                        pipe.x + game.pipeWidth < game.bird.x - game.bird.radius
                    ) {
                        game.scored.add(pipe.id);
                        score++;
                        if (score > highScore) {
                            highScore = score;
                            localStorage.setItem('flappyMemeHighScore', highScore);
                        }
                        playSound("score");
                    }

                    // Check collision
                    const birdLeft = game.bird.x - game.bird.radius;
                    const birdRight = game.bird.x + game.bird.radius;
                    const birdTop = game.bird.y - game.bird.radius;
                    const birdBottom = game.bird.y + game.bird.radius;

                    if (
                        birdRight > pipe.x &&
                        birdLeft < pipe.x + game.pipeWidth &&
                        (birdTop < pipe.gapY || birdBottom > pipe.gapY + game.pipeGap)
                    ) {
                        gameState = "gameOver";
                        playSound("die");
                        // Stop music on game over
                        if (customAudioRef) {
                            customAudioRef.pause();
                            customAudioRef.currentTime = 0;
                        }
                        return false;
                    }

                    return pipe.x > -game.pipeWidth;
                });

                // Draw bird
                ctx.save();
                ctx.translate(game.bird.x, game.bird.y);
                ctx.rotate(Math.min(game.bird.velocity * 0.05, Math.PI / 4));

                if (customText.trim()) {
                    ctx.globalAlpha = 1.0;
                    ctx.font = `bold ${game.bird.radius * 1.5}px Arial`;
                    ctx.textAlign = "center";
                    ctx.textBaseline = "middle";
                    ctx.fillStyle = currentTheme.title;
                    ctx.lineWidth = 3;
                    ctx.strokeText(customText, 0, 0);
                    ctx.fillText(customText, 0, 0);
                } else if (customImage && imageRef) {
                    ctx.globalAlpha = 1.0;
                    const size = game.bird.radius * 2;
                    ctx.drawImage(imageRef, -size / 2, -size / 2, size, size);
                } else {
                    ctx.globalAlpha = 1.0;
                    ctx.font = `${game.bird.radius * 2}px Arial`;
                    ctx.textAlign = "center";
                    ctx.textBaseline = "middle";
                    ctx.fillText(MEME_FACES[currentMeme].emoji, 0, 0);
                }
                ctx.restore();

                // Check ground/ceiling collision
                if (
                    game.bird.y + game.bird.radius > 500 ||
                    game.bird.y - game.bird.radius < 0
                ) {
                    gameState = "gameOver";
                    playSound("die");
                    // Stop music on game over
                    if (customAudioRef) {
                        customAudioRef.pause();
                        customAudioRef.currentTime = 0;
                    }
                    return;
                }

                animationFrameRef = requestAnimationFrame(gameLoop);
            };

            animationFrameRef = requestAnimationFrame(gameLoop);
        }

        function render() {
            const root = document.getElementById('root');
            const currentTheme = getCurrentTheme();

            root.innerHTML = `
                <div class="min-h-screen bg-gradient-to-b ${currentTheme.bg} flex flex-col items-center justify-center p-4" style="background: linear-gradient(to bottom, ${currentTheme.canvasGradient[0]}, ${currentTheme.canvasGradient[2]});">
                    <div class="text-center mb-4">
                        <h1 class="text-4xl md:text-6xl font-bold mb-2" style="text-shadow: 4px 4px 0 rgba(0,0,0,0.5); color: ${currentTheme.title};">
                            üéÆ FLAPPY MEME üéÆ
                        </h1>
                        <div class="flex gap-4 md:gap-8 justify-center text-lg md:text-2xl font-bold" style="color: ${currentTheme.text};">
                            <div>Score: ${score}</div>
                            <div>High Score: ${highScore}</div>
                        </div>
                        <div class="mt-2 text-sm md:text-lg" style="color: ${currentTheme.textLight};">
                            Playing as: ${
                                customText.trim()
                                    ? `"${customText}" ‚úçÔ∏è`
                                    : customImage
                                        ? "Custom Image üñºÔ∏è"
                                        : `${MEME_FACES[currentMeme].name} ${MEME_FACES[currentMeme].emoji}`
                            }
                        </div>
                    </div>

                    <div class="relative w-full max-w-[800px]">
                        <canvas
                            id="gameCanvas"
                            width="800"
                            height="500"
                            class="rounded-lg shadow-2xl cursor-pointer w-full"
                            style="border: 4px solid ${currentTheme.canvasBorder}; display: block; margin: 0 auto; max-width: 100%; height: auto;"
                        ></canvas>

                        ${gameState === "start" ? `
                            <div class="absolute inset-0 flex flex-col items-center justify-center bg-black bg-opacity-60 rounded-lg">
                                <div class="text-center px-4">
                                    <div class="text-6xl md:text-8xl mb-4">
                                        ${customText.trim() ? customText : customImage ? "üñºÔ∏è" : MEME_FACES[currentMeme].emoji}
                                    </div>
                                    <div class="text-2xl md:text-4xl font-bold mb-6" style="color: ${currentTheme.title};">
                                        Ready to Flap?
                                    </div>
                                    <button id="startBtn" class="px-8 md:px-12 py-4 md:py-5 rounded-lg font-bold text-2xl md:text-3xl transition-all shadow-lg mb-4"
                                        style="background-color: ${currentTheme.button}; color: ${currentTheme.title}; border: 3px solid ${currentTheme.buttonBorder}; cursor: pointer;">
                                        ‚ñ∂Ô∏è START GAME
                                    </button>
                                    <div class="text-lg md:text-xl mt-4" style="color: ${currentTheme.text};">
                                        Or press SPACE / tap canvas to play
                                    </div>
                                </div>
                            </div>
                        ` : ''}

                        ${gameState === "gameOver" ? `
                            <div class="absolute inset-0 flex items-center justify-center bg-black bg-opacity-80 rounded-lg">
                                <div class="text-center px-4">
                                    <div class="text-6xl md:text-8xl mb-4">üíÄ</div>
                                    <div class="text-3xl md:text-5xl font-bold mb-4" style="color: ${currentTheme.title};">
                                        GAME OVER!
                                    </div>
                                    <div class="text-2xl md:text-3xl mb-6" style="color: ${currentTheme.text};">
                                        Score: ${score}
                                    </div>
                                    <button id="retryBtn" class="px-8 md:px-12 py-3 md:py-4 rounded-lg font-bold text-xl md:text-2xl transition-colors shadow-lg"
                                        style="background-color: ${currentTheme.button}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.buttonBorder};">
                                        üîÑ RETRY
                                    </button>
                                </div>
                            </div>
                        ` : ''}
                    </div>

                    <div class="mt-4 flex flex-wrap gap-2 md:gap-4 justify-center max-w-[800px]">
                        <button id="themeBtn" class="px-4 md:px-8 py-2 md:py-3 rounded-lg font-bold text-base md:text-xl transition-colors shadow-lg"
                            style="background-color: ${currentTheme.button}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.buttonBorder};">
                            ${theme === "night" ? "‚òÄÔ∏è Day" : "üåô Night"}
                        </button>

                        ${!customImage && !customText.trim() ? `
                            <button id="changeMemeBtn" class="px-4 md:px-8 py-2 md:py-3 rounded-lg font-bold text-base md:text-xl transition-colors shadow-lg"
                                style="background-color: ${currentTheme.button}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.buttonBorder};">
                                <span class="hidden md:inline">Change Meme ${MEME_FACES[currentMeme].emoji}</span>
                                <span class="md:hidden">Change ${MEME_FACES[currentMeme].emoji}</span>
                            </button>
                        ` : ''}

                        <button id="textBtn" class="px-4 md:px-8 py-2 md:py-3 rounded-lg font-bold text-base md:text-xl transition-colors shadow-lg"
                            style="background-color: ${currentTheme.button}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.buttonBorder};">
                            ‚úçÔ∏è Text
                        </button>

                        <label class="px-4 md:px-8 py-2 md:py-3 rounded-lg font-bold text-base md:text-xl transition-colors shadow-lg cursor-pointer"
                            style="background-color: ${currentTheme.button2}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.button2Border};">
                            <span class="hidden md:inline">üì∏ Upload Face</span>
                            <span class="md:hidden">üì∏ Face</span>
                            <input type="file" accept="image/*" id="imageInput" class="hidden" />
                        </label>

                        <label class="px-4 md:px-8 py-2 md:py-3 rounded-lg font-bold text-base md:text-xl transition-colors shadow-lg cursor-pointer"
                            style="background-color: ${currentTheme.button2}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.button2Border};">
                            <span class="hidden md:inline">üéµ Upload Music</span>
                            <span class="md:hidden">üéµ Music</span>
                            <input type="file" accept="audio/*" id="musicInput" class="hidden" />
                        </label>

                        ${customImage || customText.trim() ? `
                            <button id="resetBtn" class="px-4 md:px-8 py-2 md:py-3 rounded-lg font-bold text-base md:text-xl transition-colors shadow-lg"
                                style="background-color: ${currentTheme.buttonDanger}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.buttonDangerBorder};">
                                <span class="hidden md:inline">‚ùå Reset to Meme</span>
                                <span class="md:hidden">‚ùå Reset</span>
                            </button>
                        ` : ''}
                    </div>

                    ${showTextInput ? `
                        <div class="mt-4 flex gap-2 items-center px-4 w-full max-w-[500px]">
                            <input
                                type="text"
                                id="textInput"
                                value="${customText}"
                                placeholder="Enter your text..."
                                maxlength="10"
                                class="flex-1 px-4 py-2 rounded-lg text-lg md:text-xl font-bold"
                                style="background-color: ${currentTheme.button}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.buttonBorder}; outline: none;"
                                autofocus
                            />
                            <button id="submitTextBtn" class="px-4 md:px-6 py-2 rounded-lg font-bold text-lg md:text-xl transition-colors shadow-lg"
                                style="background-color: ${currentTheme.button2}; color: ${currentTheme.title}; border: 2px solid ${currentTheme.button2Border};">
                                ‚úÖ
                            </button>
                        </div>
                    ` : ''}

                    <div class="mt-4 text-center px-4" style="color: ${currentTheme.textLight};">
                        <div class="text-xs md:text-sm opacity-75">
                            Desktop: Press SPACE to jump
                        </div>
                        <div class="text-xs md:text-sm opacity-75">
                            Mobile: Tap anywhere to jump
                        </div>
                        ${customMusic ? '<div class="text-xs md:text-sm opacity-75 mt-2">üéµ Playing your custom music!</div>' : ''}
                    </div>
                </div>
            `;

            // Attach event listeners
            const canvas = document.getElementById('gameCanvas');
            if (canvas) {
                canvas.addEventListener('click', () => gameState !== 'gameOver' && jump());
            }

            document.getElementById('startBtn')?.addEventListener('click', startGame);
            document.getElementById('themeBtn')?.addEventListener('click', toggleTheme);
            document.getElementById('changeMemeBtn')?.addEventListener('click', changeMeme);
            document.getElementById('textBtn')?.addEventListener('click', () => {
                showTextInput = !showTextInput;
                render();
            });
            document.getElementById('retryBtn')?.addEventListener('click', startGame);
            document.getElementById('imageInput')?.addEventListener('change', handleImageUpload);
            document.getElementById('musicInput')?.addEventListener('change', handleMusicUpload);
            document.getElementById('resetBtn')?.addEventListener('click', () => {
                customImage = null;
                customText = "";
                imageRef = null;
                render();
            });
            document.getElementById('textInput')?.addEventListener('keypress', (e) => {
                if (e.key === 'Enter') {
                    if (document.getElementById('textInput').value.trim()) {
                        customText = document.getElementById('textInput').value;
                        showTextInput = false;
                        render();
                    }
                }
            });
            document.getElementById('submitTextBtn')?.addEventListener('click', () => {
                customText = document.getElementById('textInput').value;
                if (customText.trim()) {
                    showTextInput = false;
                    render();
                }
            });
        }

        // Initialize
        window.addEventListener('load', () => {
            if (typeof window !== "undefined") {
                audioContextRef = new (window.AudioContext || window.webkitAudioContext)();
            }
            render();
            setTimeout(() => {
                drawCanvasBackground();
            }, 10);
        });

        window.addEventListener('keydown', (e) => {
            if (e.code === 'Space') {
                e.preventDefault();
                jump();
            }
        });

        // Handle canvas resize - DISABLED to keep fixed 800x500 coordinates
        // function updateCanvasSize() {
        //     const maxWidth = Math.min(window.innerWidth - 32, 800);
        //     const aspectRatio = 800 / 500;
        //     const width = maxWidth;
        //     const height = width / aspectRatio;
        //     canvasSize = { width, height };
        //     render();
        // }

        // window.addEventListener('resize', updateCanvasSize);
    </script>
</body>
</html>
